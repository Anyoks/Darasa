<!DOCTYPE html>
<html>
<head>
  <title>DarasaApp</title>
  <%= stylesheet_link_tag    "application", media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "application", 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
</head>
<body>

<!-- User Login  -->
<% if user_signed_in? %>
	<%= render 'nav' %>
	<% else %>
	   
<%end%>






<div class='container'>	
	<%= content_tag :div, alert, class: "alert-danger"%>
	<%= content_tag :div, notice, class: "alert-success"%>
	<%= yield %>
</div>




</body>
</html>